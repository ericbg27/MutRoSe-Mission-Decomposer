{
    "world_db": {
        "type": "file",
        "file_type": "xml",
        "path": "knowledge/Object Picking/Default/World_db.xml",
        "xml_root": "world_db"
    },

    "output": {
        "output_type": "file",
        "file_path": "output/Object Picking/Default/task_output.json",
        "file_type": "json"
    },

    "location_types": ["Room"],

    "type_mapping": [
        {
            "hddl_type": "room",
            "ocl_type": "Room"
        },
        {
            "hddl_type": "obj",
            "ocl_type": "Object"
        }
    ],

    "var_mapping": [
        {
            "task_id": "AT1",
            "map": [
                {
                    "gm_var": "current_room",
                    "hddl_var": "?rm"
                },
                {
                    "gm_var": "obj",
                    "hddl_var": "?o"
                }
            ]
        }
    ],

    "semantic_mapping": [
        {
            "type": "attribute",
            "name": "is_picked",
            "relates_to": "Object",
            "belongs_to": "world_db",
            "mapped_type": "predicate",
            "map": {
                "pred": "picked",
                "arg_sorts": ["obj"]
            }
        }
    ]
}